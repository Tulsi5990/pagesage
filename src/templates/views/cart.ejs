<!DOCTYPE html>
<html>
<head>
  <title>Cart</title>
  <!-- Add your CSS and JavaScript references -->
</head>
<body>
  <h1>Your Cart</h1>

  <% if (cartItems.length > 0) { %>
    <ul>
      <% cartItems.forEach((item) => { %>
        <li><%= item %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>Your cart is empty.</p>
  <% } %>

  <!-- Add other cart-related content -->

</body>
</html>

